{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://127.0.0.1:*/*" }]
    },
    "core:default",
    "log:default",
    "http:allow-fetch",
    "store:default",
    "http:allow-fetch-send"
  ]
}
